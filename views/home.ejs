<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function (post) { %>
    <h2><%= post.blogTitle %></h2>
    <p>
        <%= post.blogText.slice(0,100) + "..."%>
        <a href="/posts/<%= post.blogTitle %>">Read More</a>
    </p>
<% }); %>
<%- include("partials/footer") -%>
